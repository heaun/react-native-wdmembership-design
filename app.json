{
  "expo": {
    "name": "WDMembershipApp",
    "slug": "WDMembershipApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B2B2B"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/NanumSquareNeo-bRg.ttf",
            "./src/assets/fonts/NanumSquareNeo-cBd.ttf",
            "./src/assets/fonts/NanumSquareNeo-dEb.ttf",
            "./src/assets/fonts/NanumSquareNeo-eHv.ttf",
            "./src/assets/fonts/NanumSquareNeo-aLt.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agnes7.WDMembershipApp",
      "fonts": ["NanumSquareNeo-bRg", "NanumSquareNeo-cBd", "NanumSquareNeo-dEb", "NanumSquareNeo-eHv", "NanumSquareNeo-aLt"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#2B2B2B"
      },
      "fonts": [
        {
          "fontFamily": "NanumSquareNeo-bRg",
          "fontDefinitions": [
            {
              "path": "./src/assets/fonts/NanumSquareNeo-bRg.ttf",
              "fontFamily": "NanumSquareNeo-bRg",
              "fontWeight": "400",
              "fontStyle": "normal"
            },
            {
              "path": "./src/assets/fonts/NanumSquareNeo-cBd.ttf",
              "fontFamily": "NanumSquareNeo-cBd",
              "fontWeight": "700",
              "fontStyle": "normal"
            },
            {
              "path": "./src/assets/fonts/NanumSquareNeo-dEb.ttf",
              "fontFamily": "NanumSquareNeo-dEb",
              "fontWeight": "800",
              "fontStyle": "normal"
            }
          ]
        },
        {
          "fontFamily": "NanumSquareNeo-cBd",
          "fontWeight": "bold"
        }
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.wiivlink.WDMembershipApp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
